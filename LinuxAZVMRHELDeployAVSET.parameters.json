{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "SSHAdminUsername": {
      "value": "<Enter SSHAdmin Username>"
    },
    "vmName": {
      "value": "<Enter VM Name>"
    },
    "vmSize": {
      "value": "<Enter VM Size>"
    },
    "linuxVersion": {
      "value": "7.3"
    },
    "numberOfDataDisks": {
      "value": 1
    },
    "DataDiskSize": {
      "value": 128
    },
    "vnetname": {
      "value": "<enter target vnet name>"
    },
    "vnetresourcegrp": {
      "value": "<enter target vnet resource group name>"
    },
    "subnetname": {
      "value": "<enter target subnet name>"
    },
	  "CostCenter": {
      "value": "<Enter 3-digit cost code>"
    },
	  "BusinessUnit": {
      "value": "<Enter business unit name>"
    },
	  "Product": {
      "value": "<Enter product name>"
    },
    "Application": {
      "value": "<enter application type/name>"
    },
    "Environment": {
      "value": "<Enter environment type, i.e. non-prod>"
    },
	  "SupportTeam": {
      "value": "<Enter e-mail address of support team>"
    },
    "vhdStorageType": {
      "value": "Standard_LRS"
    },
    "AvailabilitySetName": {
      "value": "<availability set name>"
    }
  }
}